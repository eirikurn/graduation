{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "ecmaFeatures": {
      "modules": false
  },
  "rules": {
    "semi": [2, "never"],
    "max-len": [0],
    "eqeqeq": [2, "allow-null"],
    "no-cond-assign": [2, "except-parens"],
    "react/jsx-curly-spacing": [1, "never"],
    "react/require-extension": [1, { "extensions": [".js"] }],
    "no-use-before-define": [0],
    "import/no-unresolved": [0]
  },
  "globals": {
    "describe": true,
    "it": false
  }
}
